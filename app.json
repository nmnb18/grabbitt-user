{
  "expo": {
    "name": "Grabbitt Seller",
    "slug": "grabbitt-seller",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "grabbittseller",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grabbitt.seller",
      "associatedDomains": [
        "applinks:grabbittseller.page.link"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-512.png",
        "backgroundImage": "./assets/images/android-512.png",
        "monochromeImage": "./assets/images/android-258.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.grabbitt.seller",
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "grabbittseller",
              "host": "*"
            }
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7a31674d-15f2-485a-bdc1-3273d89e72e0"
      }
    },
    "owner": "spectra123"
  }
}
